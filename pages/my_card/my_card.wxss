.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}
.gooditem{
  background: #ffffff;
  display: block;
  margin-right: 8px;
  margin-left: 8px;
  margin-bottom: 8px;
  border-radius:8px 
}
.gooditem_shop{
  display:block;
  width: 100%;
  height: 96px;
  background: #ffffff;
}
.gooditem_goodslist{
  display: block;
  overflow: hidden;
  background: #fcfcfc;
}
.gooditem_logo ,.gooditem_logo image{
  width: 80px;
  height: 80px;
  border-radius: 40px;
}
.gooditem_logo{
  float: left;
  margin: 8px;
}
.gooditem_info{
  float: left;
  margin: 8px;
}
.gooditem_lbs{
  float: right;
  margin: 8px;
  width: 120px;
}
.shop_title, .shop_add{
  height: 40px;
  line-height: 40px;
  text-align: left;
  display: block;
}
.shop_title{
  font-size: 18px;
}
.shop_add{
  font-size: 16px;
}
.goods{
  display: block;
  width: 100%;
  height: 32px;
}
.goods_title{
  height: 32px;
  line-height: 32px;
  text-align: left;
  display: block;
  float: left;
  margin-left: 8px;
  font-size: 12px;
  color: #ccc;
}
.goods_integral{
  height: 32px;
  line-height: 32px;
  text-align: right;
  display: block;
  float: right;
  margin-right: 8px;
}