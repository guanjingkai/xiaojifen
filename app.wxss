@import 'style/weui.wxss';

@font-face {
	font-family: 'iconfont';
	src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
	font-weight: normal;
	font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.icon-location::before{
  content: "\e667"
}
.icon-search::before{
  content: "\e620"
}
page {
  /* background-color: #F8F8F8; */
  background-color: #fff;
  font-size: 16px;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.page__hd {
  padding: 40px;
}

.page__bd {
  padding-bottom: 40px;
}

.page__bd_spacing {
  padding-left: 15px;
  padding-right: 15px;
}

.page__ft {
  padding-bottom: 10px;
  text-align: center;
}

.page__title {
  text-align: left;
  font-size: 20px;
  font-weight: 400;
}

.page__desc {
  margin-top: 5px;
  color: #888;
  text-align: left;
  font-size: 14px;
}

.bigGoodsTitle {
  width: 210px;
  height: 24px;
  line-height: 24px;
  display: block;
  font-size: 14px;
  float: left;
}

.bigTitle {
  display: flex;
  height: 56px;
  width: 100%;
  background: #fff;
}

.bigTitleV {
  width: 100%;
  margin-left: 16px;
}

.bigTitle1 {
  width: 100%;
  display: block;
  height: 32px;
  line-height: 32px;
  font-size: 20px;
}

.bigTitle2 {
  width: 100%;
  display: block;
  height: 24px;
  line-height: 24px;
  font-size: 14px;
  color: #777;
}

.bigTitleM {
  float: right;
  width: 100px;
}

.bigTitleM {
  height: 56px;
  line-height: 56px;
  font-size: 16px;
  color: #777;
}

.bigGoodsBox {
  width: 100%;
  white-space: nowrap;
  display: flex;
  height: 176px;
  margin-bottom: 16px;
  border-bottom: 1px solid #eee;
  padding-bottom: 16px;
  overflow: scroll;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.bigGoods {
  width: 210px;
  height: 168px;
  padding: 8px;
  border-radius: 16px;
  background: #fff;
  margin-right: 8px;
  display: inline-block;
  overflow: hidden;
}

.bigImage {
  width: 210px;
  height: 120px;
}

.bigImage image {
  width: 210px;
  height: 120px;
  border-radius: 8px;
}

.bigGoodsInfo {
  width: 210px;
  height: 48px;
}
.bigGoodsFen{
  float: left;
  height: 24px;
}
.bigGoodsHuiB{
  font-size: 14px;
  color: #FE8800; 
}
.bigGoodsPrice{
  font-size: 14px;
  color: #777;
}
.huibi {
  color: orange;
  font-size: 16px;
}
/*轮播图  */

.slide, .slide-item {
  width: 100%;
  height: 240px;
}

.slide-image-box {
  width: 94%;
  margin: 8px auto;
  height: 180px;
}

.slide-image {
  width: 100%;
  height: 180px;
  border-radius: 16px;
}

.slide-title {
  height: 40px;
  width: 100%;
}

.slide-title-1 {
  height: 24px;
  line-height: 24px;
  font-size: 16px;
  width: 100%;
  margin-left: 8px;
  display: block;
}

.slide-title-2 {
  height: 16px;
  line-height: 16px;
  font-size: 12px;
  color: #777;
  width: 100%;
  margin-left: 8px;
  display: block;
}

.weui-navbar {
  background: #fff;
}
/*地图  */
.add-box{
  height: 44px;
  width: 100%;
  display: flex;
  border-top:1px solid #efefef;
  border-bottom: 1px solid #efefef;
  background: #fff;
}
.add-location{
  width: 100%;
  height: 44px;
  line-height: 44px;
  border-right: 1px solid #efefef;
}
.add-location text{
  line-height: 44px;
  margin-left: 16px;
  color: #777;
  font-size: 14px;
}
.add-map{
  height: 44px;
  width: 44px;
}
/* 商品列表 */

.gooditem {
  background: #fff;
  display: block;
  width: 46%;
  margin: 16rpx 2%;
  overflow: hidden;
  float: left;
  position: relative;
}

.gooditem_shop {
  display: block;
  width: 100%;
  height: 192rpx;
  background: #fff;
}

.gooditem_goodslist {
  display: block;
  overflow: hidden;
  background: #fcfcfc;
}

.gooditem_logo, .gooditem_logo image {
  width: 100%;
  height: 320rpx;
}
.gooditem_logo image {
  width: 100%;
  height: 320rpx;
}

.gooditem_info {
  float: left;
  height: 64rpx;
  line-height: 64rpx;
  margin-left: 16rpx;
}

.gooditem_lbs {
  position: absolute;
  top: 0px;
  right:0px;
  width: 120rpx;
  line-height: 40rpx;
  height: 40rpx;
  background: #88d987;
  color: #fff;
  text-align: center;
}
.goods {
  display: flex;
  width: 100%;
  height: 48rpx;
  float: left;
  font-size: 28rpx;
  color: #777;
}


.goods_integral {
  height: 48rpx;
  line-height: 48rpx;
  width: 200rpx;
  text-align: right;
  display: block;
  margin-right: 16rpx;
  color: #FE8800;
}
.goods_title {
  height: 48rpx;
  line-height: 48rpx;
  width: 100%;
  text-align: left;
  margin-left: 16rpx;
  display: block;
}
/*tabbar  */
.weui-cell {
  padding: 24rpx 30rpx 24rpx 70rpx;
}
.weui-tab__panel {
    background: #efefef;
}
.weui-tab__content {
  text-align: center;
}
.weui-navbar{
  position: fixed;
  top: 0px;
  left: 0px;
}